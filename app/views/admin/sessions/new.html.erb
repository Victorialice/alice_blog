<div class="row" ng-controller="AdminSessionsController">
<div class="small-12 large-8 columns">
<h3 class="blog-title"><%= ::Temple::Utils.escape_html((t('admin.session.title'))) %></h3>
<% _slim_controls1 = form_tag admin_sessions_path do
 %><div class="row">
<div class="small-12 large-8 columns">
<%= ::Temple::Utils.escape_html((label_tag 'username', t('admin.session.username'))) %>
<%= ::Temple::Utils.escape_html((text_field_tag 'username', params[:username], placeholder: t('admin.session.username_placeholder'))) %>
<%= ::Temple::Utils.escape_html((label_tag 'username', t('admin.session.password'))) %>
<%= ::Temple::Utils.escape_html((password_field_tag 'password', params[:password], placeholder: t('admin.session.password_placeholder'))) %>

</div></div><button class="button" type="submit"><%= ::Temple::Utils.escape_html((t('admin.session.login_button'))) %></button>
<% end %><%= ::Temple::Utils.escape_html((_slim_controls1)) %></div></div>
